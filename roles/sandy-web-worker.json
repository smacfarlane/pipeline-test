{
  "name": "sandy-web-worker",
  "description": "Sandy Workers",
  "json_class": "Chef::Role",
  "default_attributes": {
  },
  "override_attributes": {
    "chruby": {
      "default": "2.1.1",
      "rubies": {
        "1.9.3-p392": false,
        "1.9.3-p448": false,
        "2.1.1": true
      },
      "auto_switch": false
    },
    "users": [
      "webdev",
      "processing"
    ],
    "iptables-ng": {
      "rules": {
        "filter": {
          "INPUT": {
            "22-ssh": {
              "rule": "-m state --state NEW -p tcp -s 137.229.19.0/24,10.19.16.0/23,10.0.19.0/24,137.229.87.0/24 --dport 22 -j ACCEPT",
              "ip_version": 4
            }
          }
        }
      }
    }
  },
  "chef_type": "role",
  "run_list": [
    "sandy::worker"
  ],
  "env_run_lists": {
  }
}
