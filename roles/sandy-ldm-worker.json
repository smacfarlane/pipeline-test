{
  "name": "sandy-ldm-worker",
  "description": "Sandy LDM Worker",
  "json_class": "Chef::Role",
  "default_attributes": {
  },
  "override_attributes": {
    "sandy": {
      "worker": {
        "queues": ["ldm", "default"]
      }
    },
    "ldm": {
      "allows": [{
        "feedset": "EXP",
        "hostname": "ldm.gina.alaska.edu"
      }],
      "global_queue": true
    },
    "iptables-ng": {
      "rules": {
        "filter": {
          "INPUT": {
            "388-ldm-tcp": {
              "rule": "-m state --state NEW -p tcp -s 137.229.19.0/24,10.19.16.0/23 --dport 388 -j ACCEPT",
              "ip_version": 4
            },
            "388-ldm-ucp": {
              "rule": "-m state --state NEW -p udp -s 137.229.19.0/24,10.19.16.0/23 --dport 388 -j ACCEPT",
              "ip_version": 4
            }
          }
        }
      }
    }
  },
  "chef_type": "role",
  "run_list": [
    "role[sandy-base]",
    "role[sandy-worker]",
    "ldm::default"
  ],
  "env_run_lists": {
  }
}
